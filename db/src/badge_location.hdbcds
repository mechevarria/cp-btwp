entity BADGE_LOCATION {
    key ID : Integer generated by default as identity (start with 1 increment by 1);
    LOC: String(100);
    PROB: String(100);
    SEEN: String(100);
    FLOOR: Integer;
    DEVICE_ID: String(100);
    LAT: Double;
    LNG: Double;
    SEEN_TS: UTCTimestamp;
    ALTITUDE: Double;
};
